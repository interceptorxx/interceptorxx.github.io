#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/lighthouse@11.3.0/node_modules/lighthouse/core/scripts/node_modules:/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/lighthouse@11.3.0/node_modules/lighthouse/core/node_modules:/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/lighthouse@11.3.0/node_modules/lighthouse/node_modules:/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/lighthouse@11.3.0/node_modules:/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/lighthouse@11.3.0/node_modules/lighthouse/core/scripts/node_modules:/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/lighthouse@11.3.0/node_modules/lighthouse/core/node_modules:/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/lighthouse@11.3.0/node_modules/lighthouse/node_modules:/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/lighthouse@11.3.0/node_modules:/Users/insidious/Documents/Developer Insight/alpha/svelte-template/myapp-qa.github.io/myapp-qa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lighthouse/core/scripts/manual-chrome-launcher.js" "$@"
else
  exec node  "$basedir/../lighthouse/core/scripts/manual-chrome-launcher.js" "$@"
fi
